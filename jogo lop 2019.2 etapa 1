var x = 200;
var y = 30;

function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  
  if(keyIsDown(LEFT_ARROW) && x >= 60){
     x-= 10;
     }
  if(keyIsDown(RIGHT_ARROW) && x <= 340){
    x += 10;
  }
  if(keyIsDown(UP_ARROW) && y >= 30){
    y -= 10;
  }
  if(keyIsDown(DOWN_ARROW) && y <= 370){
     y += 5;
     }
  
  ellipse(x,y,100,50)
  square(100,30,80)
}
